@model RCM.Application.ViewModels.ServicoViewModel

@{
    ViewData["Title"] = "Relacionar Serviço";
}

<h5>Adicionar</h5>

<div class="form-container col s12 m12">
    <form asp-action="AttachServico">
        @Html.EditorForModel()

        <div class="row pt-2 px-1">
            <post-button>Confirmar</post-button>
        </div>
    </form>
</div>

<div>
    <a asp-action="Details" asp-route-id="@Model.VendaId">Voltar para detalhes</a>
</div>